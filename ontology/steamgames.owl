    <DataPropertyAssertion>
        <DataProperty IRI="#hasText"/>
        <NamedIndividual IRI="#2990627"/>
        <Literal>This is an amazing game, but it&apos;s also pretty buggy because of being rushed. Buy it, and then use wesp&apos;s unofficial patch to fix all the bugs and optionally add in cut content and other improvements.  http://www.patches-scrolls.de/patch/4647</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#hasNumberOfHelpfulVotes"/>
        <NamedIndividual IRI="#2990628"/>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">0</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
