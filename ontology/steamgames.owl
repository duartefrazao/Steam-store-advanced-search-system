    <DataPropertyAssertion>
        <DataProperty IRI="#hasText"/>
        <NamedIndividual IRI="#4702044"/>
        <Literal>HOG lovers, rejoice---Mystery P.I. games are awesome.</Literal>
    </DataPropertyAssertion>
