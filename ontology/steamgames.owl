    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#hasText"/>
        <NamedIndividual IRI="#4847934"/>
        <Literal>Unscripted and hardcore. One of my all time favourites along with Alien Vs Predator on the Jaguar 64.</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#hasNumberOfHelpfulVotes"/>
