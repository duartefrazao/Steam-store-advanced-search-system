    <DataPropertyAssertion>
        <DataProperty IRI="#hasText"/>
        <NamedIndividual IRI="#4538475"/>
        <Literal>Do you like Diehard?  If so every time you finish a level its like receiving a High Five from Alan Rickman.  10/10 Would High Five Alan Rickman again.  Oh and something about pegs.</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#hasNumberOfHelpfulVotes"/>
        <NamedIndividual IRI="#4540671"/>
        <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">1</Literal>
    </DataPropertyAssertion>
    <DataPropertyAssertion>
        <DataProperty IRI="#hasPositiveSentiment"/>
